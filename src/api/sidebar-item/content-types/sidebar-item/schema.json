{
  "kind": "collectionType",
  "collectionName": "sidebar_items",
  "info": {
    "singularName": "sidebar-item",
    "pluralName": "sidebar-items",
    "displayName": "Products",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "Url": {
      "type": "string"
    },
    "order": {
      "type": "integer"
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "isActive": {
      "type": "boolean"
    },
    "parent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sidebar-item.sidebar-item",
      "inversedBy": "children"
    },
    "children": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sidebar-item.sidebar-item",
      "mappedBy": "parent"
    },
    "PD_Description": {
      "type": "blocks"
    },
    "PD_PriceRange": {
      "type": "string"
    },
    "PD_Gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "quantity": {
      "type": "string"
    },
    "PD_Description2": {
      "type": "blocks"
    },
    "PD_Points": {
      "type": "text"
    },
    "isHomeProduct": {
      "type": "boolean"
    },
    "VAT": {
      "type": "decimal",
      "default": 20,
      "max": 20,
      "min": 5
    },
    "weight": {
      "type": "decimal"
    },
    "numberOfBoxes": {
      "private": true,
      "type": "decimal"
    }
  }
}
