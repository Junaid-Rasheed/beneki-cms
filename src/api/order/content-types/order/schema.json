{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "orderNumber": {
      "type": "uid"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "totalAmount": {
      "type": "decimal"
    },
    "paymentMethod": {
      "type": "enumeration",
      "enum": [
        "BnpAxepta",
        "Paypal",
        "WireTransfer"
      ]
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item"
    },
    "orderStatus": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Paid",
        "Shipped",
        "Completed"
      ]
    },
    "user_address": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-address.user-address"
    },
    "notes": {
      "type": "string"
    },
    "transactionId": {
      "type": "string"
    }
  }
}
